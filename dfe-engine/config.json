{
    "data_dir": "files",
    "files_dir": "files/data/nfe",
    "url": "https://api.arquivei.com.br/",
    "endpoints": {
        "nfe/received": {
            "endpoint": "v1/nfe/received",
            "dir": "xml",
            "extension": "xml",
            "verb": "get",
            "limit": "50",
            "cursor": 250
        },
        "nfe/events": {
            "endpoint": "v2/nfe/events",
            "dir": "event",
            "extension": "xml",
            "verb": "get",
            "limit": "0",
            "cursor": 0
        },
        "nfe/danfe": {
            "endpoint": "v1/nfe/danfe",
            "dir": "danfe",
            "extension": "pdf",
            "verb": "get",
            "limit": "0",
            "cursor": 0
        }
    },
    "key_parser": {
        "nfe": {
            "dt_issue": "[\"nfeProc\"][\"NFe\"][\"infNFe\"][\"ide\"][\"dhEmi\"]",
            "issuer_code": "[\"nfeProc\"][\"NFe\"][\"infNFe\"][\"emit\"][\"CNPJ\"]",
            "receipt_code": "[\"nfeProc\"][\"NFe\"][\"infNFe\"][\"dest\"][\"CNPJ\"]"
        }
    }
}