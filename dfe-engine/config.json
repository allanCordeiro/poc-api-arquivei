{
    "data_dir": "data",
    "xml_dir": "data/nfe",
    "url": "https://api.arquivei.com.br/",
    "endpoints": {
        "nfe/received": {
            "endpoint": "v1/nfe/received",
            "dir": "xml",
            "extension": "xml",
            "verb": "get",
            "limit": "50",
            "cursor": 100
        },
        "nfe/events": {
            "endpoint": "v2/nfe/events",
            "dir": "event",
            "extension": "xml",
            "verb": "get",
            "limit": "0",
            "cursor": 0
        },
        "nfe/danfe": {
            "endpoint": "v1/nfe/danfe",
            "dir": "danfe",
            "extension": "pdf",
            "verb": "get",
            "limit": "0",
            "cursor": 0
        }
    }
}